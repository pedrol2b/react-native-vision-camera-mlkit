{
  "name": "react-native-vision-camera-mlkit",
  "version": "1.0.0",
  "description": "A powerful React Native Vision Camera plugin delivering high-performance Google ML Kit frame processor featuresâ€”including text recognition (OCR), face detection, barcode scanning, pose detection, and more. Seamlessly bridges native ML Kit capabilities for real-time, on-device computer vision in your React Native apps.",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-vision-camera-mlkit-example",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it --only-version",
    "test": "jest"
  },
  "keywords": [
    "android",
    "ios",
    "ocr",
    "react-native",
    "barcode",
    "vision",
    "mlkit",
    "google-ml-kit",
    "vision-camera",
    "frame-processor",
    "machine-learning",
    "computer-vision",
    "react-native-module",
    "native-module",
    "image-processing",
    "real-time",
    "camera",
    "on-device",
    "text-detection",
    "face-detection",
    "barcode-scanner",
    "segmentation",
    "pose-detection",
    "document-scanner"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrol2b/react-native-vision-camera-mlkit.git"
  },
  "author": "Pedro Bueno <pedrolbb.e@gmail.com> (https://github.com/pedrol2b)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pedrol2b/react-native-vision-camera-mlkit/issues"
  },
  "homepage": "https://github.com/pedrol2b/react-native-vision-camera-mlkit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native-community/cli": "20.0.1",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "^0.81.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.0",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "lefthook": "^2.0.3",
    "prettier": "^2.8.8",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.40.13",
    "react-native-vision-camera": "^4.7.3",
    "react-native-worklets-core": "^1.6.2",
    "release-it": "^19.0.4",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-vision-camera": "*",
    "react-native-worklets-core": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "VisionCameraMLKitSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.visioncameramlkit"
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "turbo-module",
    "tools": [
      "eslint",
      "lefthook",
      "release-it",
      "jest"
    ],
    "version": "0.55.1"
  }
}
